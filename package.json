{
  "name": "devdocs-desktop",
  "version": "1.0.0",
  "description": "DevDocs Desktop",
  "main": "./main.js",
  "author": "Marco Ant√¥nio",
  "license": "MIT",
  "homepage": "https://github.com/nosebit/devdocs.git",
  "repository": {
    "type": "git",
    "url": "https://github.com/nosebit/devdocs.git"
  },
  "scripts": {
    "compile": "tsc main.ts --outDir .",
    "start": "yarn compile && electron .",
    "clean": "rm -rf builds/*",
    "build": "yarn compile && yarn clean && node ./build.js",
    "icon": "sips -z 128 128 -s format $f ./images/base.png --out ./images/icon.$f",
    "icon:win": "f=ico yarn icon",
    "icon:mac": "f=icns yarn icon",
    "icon:linux": "f=png yarn icon",
    "generate:icons": "yarn icon:mac; yarn icon:win; yarn icon:linux"
  },
  "dependencies": {
    "electron": "^1.8.2"
  },
  "devDependencies": {
    "electron-packager": "^11.0.1"
  }
}
